generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserInfo {
  clerkId   String    @id
  username  String?
  name      String?
  email     String    @unique
  userSince DateTime  @default(now())
  updatedAt DateTime  @updatedAt
}
